{
    "name": "code-error-detector",
    "displayName": "AI Code Analyzer",
    "description": "AI-based code analysis with visual error explanations for Python, JavaScript, TypeScript, and more",
    "version": "1.0.0",
    "publisher": "local",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "code-error-detector.analyze",
                "title": "Analyze Code with AI"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:python",
        "onLanguage:javascript",
        "onLanguage:typescript",
        "onLanguage:javascriptreact",
        "onLanguage:typescriptreact"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/vscode": "^1.85.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "axios": "^1.6.5"
    }
}